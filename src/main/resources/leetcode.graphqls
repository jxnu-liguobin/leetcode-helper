scalar Long
scalar Object

type Query {
    question(titleSlug: String!): QuestionNode!
}

type QuestionNode {
    questionFrontendId: String!
    questionId: String!
    title: String!
    codeSnippets: [CodeSnippetNode!]
    topicTags: [TopicTagNode!]
    metaData: Object
    exampleTestcases: String
    envInfo: Object
}

type TopicTagNode {
    name: String
    slug: String
    translatedName: String
}

type CodeSnippetNode {
    lang: String!
    langSlug: String!
    code: String!
}